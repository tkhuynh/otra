<div class="container">
	<div class="dashboard-content">
		<% @host.shows.each do |show| %>
			<div class="col-xs-4">
				<div class="shadowbox">
					<div class="card-body">
					<h3 class="headers">
					<%= show.venue %>
					</h3>
					<br>
					<br>
						<% show.show_date.strftime("%m/%d/%Y") %> <br> <span><%= show.location %></span>
						<hr>
					</div>
				</div>
			</div>
		<% end %>
	</div>
</div>